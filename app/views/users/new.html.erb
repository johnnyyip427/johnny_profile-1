<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<p>new user sign up page</p>
<div>
<%=form_for(@user) do |f| %>  
  		<div> 
    		<%= f.label "first name" %><br> 
    		<%= f.text_area :first_name, :required => 'required' %>
  		</div> 
        		<div> 
    		<%= f.label "last name" %><br> 
    		<%= f.text_area :last_name, :required => 'required' %>
  		</div> 
        		<div> 
    		<%= f.label "email" %><br> 
    		<%= f.text_area :email, :required => 'required' %>
  		</div> 
        		<div> 
    		<%= f.label "password" %><br> 
    		<%= f.text_area :password, :required => 'required' %>
  		</div> 
  		<div> 
    		<%= f.submit "Submit" %> 
  		</div> 
		<% end %> 
</div>

</body>
</html>

