<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<p>new user sign up page</p>

<%= form_for(@user) do |f| %>  
  		<div> 
    		<%= f.label "first name" %><br> 
    		<%= f.text_area :first_name %>
  		</div> 
        		<div> 
    		<%= f.label "last name" %><br> 
    		<%= f.text_area :last_name %>
  		</div> 
        		<div> 
    		<%= f.label "email" %><br> 
    		<%= f.text_area :email %>
  		</div> 
        		<div> 
    		<%= f.label "password" %><br> 
    		<%= f.text_area :password %>
  		</div> 
  		<div> 
    		<%= f.submit "Submit" %> 
  		</div> 
		<% end %> 


</body>
</html>

