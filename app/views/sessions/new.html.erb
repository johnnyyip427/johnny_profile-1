<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>

<%= form_for(:session, url: login_path) do |f| %> 
  <%= f.email_field :email, :required => 'required', :placeholder => "Email" %> 
  <%= f.password_field :password, :required => 'required', :placeholder => "Password" %> 
  <%= f.submit "Log in" %>
<% end %>

</body>
</html>